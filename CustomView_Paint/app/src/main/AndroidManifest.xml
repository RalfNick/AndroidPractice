<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wang.ralf.painttest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ColorActivity" />
        <activity android:name=".activity.ColorFilterActivity" />
        <activity android:name=".activity.XfermodeActivity" />
        <activity android:name=".activity.PaintStyleActivity" />
        <activity android:name=".activity.PathEffectActivity" />
        <activity android:name=".activity.ShadowLayerViewActivity" />
        <activity android:name=".activity.FillPathViewActivity" />
        <activity android:name=".activity.MaskFilterViewActivity" />
        <activity android:name=".activity.TextPathViewActivity"></activity>
    </application>

</manifest>